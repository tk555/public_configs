set nocompatible


" other
set t_ut=""
set fenc=utf-8
set nu
syntax on
set showmatch
set noswapfile
set nobackup
set shiftwidth=4
set backspace=indent,eol,start
set belloff=all
colorscheme darkblue
hi clear CursorLine
hi CursorLine term=underline cterm=underline gui=underline ctermfg=NONE ctermbg=darkgray
" indent
set autoindent
set smartindent
" status
set title
set laststatus=2
set showcmd
" tab
set expandtab
set smarttab
set tabstop=4
" cursor
set cursorline
set cursorcolumn
" search
set hlsearch
set ignorecase
set smartcase
set wrapscan
set incsearch
